<?xml version="1.0" encoding="UTF-8"?>  
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"   
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.dream.dao.res.ImageResDao">
	<insert id="add" parameterType="org.dream.bean.res.ImageRes">
		INSERT INTO
		ImageRes(
		imageResId,
		imageItemUrl
		)
		VALUES(
		#{imageResId},
		#{imageItemUrl}
		)
	</insert>
</mapper>